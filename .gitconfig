[alias]
diff = diff --color
diffa = diff --cached
diffc = diff HEAD^
diffi = diff --ignore-all-space
diffn = !(git diff --name-only $(git merge-base HEAD main) HEAD)
diffs = !(git diff $(git merge-base HEAD main) HEAD)
comeback = checkout `git rev-list --max-parents=0 HEAD`
st=status
ch=checkout
cm=commit -m
alias=config --global -l
br=branch
rs=restore
sw=switch
tree=log --graph --oneline --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset
core.editor=code --wait
